<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/stamp_rounding">
    <stroke
        android:width="2dp"
        android:color="@color/defaultColor"/>
    <padding
        android:top="10dp"
        android:bottom="10dp"
        android:left="10dp"
        android:right="10dp" />
    <margin
        android:top="15dp"
        android:bottom="15dp"
        android:left="15dp"
        android:right="15dp" />
    <size
        android:width="40dp"
        android:height="40dp"/>
    <corners android:radius="50dp" />
    <solid android:color="#fff" />

</shape>